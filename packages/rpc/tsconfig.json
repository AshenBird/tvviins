{
  "include": [
    "src/**/*"
   ],
  "compilerOptions": {
    "target": "ESNext",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "ignoreDeprecations": "5.0",
    "composite": true,
    "lib": ["ESNext"],
    "skipLibCheck": true,
    "strict": true
  },
  "references": [
    {
      "path": "./tsconfig.node.json"
    },
    {
      "path": "./tsconfig.app.json"
    }
  ]
}
